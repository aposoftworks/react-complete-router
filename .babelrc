{
	"presets": [
		"@babel/preset-env",
		"@babel/preset-react",
		"@babel/preset-typescript"
	],
	"env": {
		"development": {
			"presets": [
				"@babel/preset-env",
				"@babel/preset-react"
			],
			"plugins": [
				"transform-es2015-modules-commonjs"
			]
		},
		"test": {
			"presets": [
				"@babel/preset-env",
				"@babel/preset-react"
			],
			"plugins": [
				"transform-es2015-modules-commonjs"
			]
		}
	},
	"plugins": [
		"@babel/plugin-transform-modules-commonjs"
	],
	"exclude": "/node_modules"
}
